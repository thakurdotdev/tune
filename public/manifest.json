{
  "name": "Tune - Your Music Buddy",
  "short_name": "Tune",
  "description": "Listen to your favorite music with Tune - your personal music streaming companion",
  "start_url": "/",
  "display": "standalone",
  "background_color": "#ffffff",
  "theme_color": "#000000",
  "orientation": "portrait-primary",
  "scope": "/",
  "lang": "en-US",
  "dir": "ltr",
  "categories": ["music", "entertainment"],
  "screenshots": [
    {
      "src": "/logo.png",
      "sizes": "512x512",
      "type": "image/png",
      "form_factor": "wide"
    }
  ],
  "icons": [
    {
      "src": "/icon-192x192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any maskable"
    },
    {
      "src": "/icon-512x512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any maskable"
    },
    {
      "src": "/logo.png",
      "sizes": "any",
      "type": "image/png",
      "purpose": "any"
    }
  ],
  "shortcuts": [
    {
      "name": "Home",
      "short_name": "Home",
      "description": "Go to home page",
      "url": "/",
      "icons": [
        {
          "src": "/icon-192x192.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Search Music",
      "short_name": "Search",
      "description": "Search for your favorite songs and artists",
      "url": "/?action=search",
      "icons": [
        {
          "src": "/icon-192x192.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "My Playlists",
      "short_name": "Playlists",
      "description": "Access your saved playlists",
      "url": "/?section=playlists",
      "icons": [
        {
          "src": "/icon-192x192.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Settings",
      "short_name": "Settings",
      "description": "Adjust your music preferences",
      "url": "/settings",
      "icons": [
        {
          "src": "/icon-192x192.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    }
  ],
  "prefer_related_applications": false,
  "protocol_handlers": [
    {
      "protocol": "web+music",
      "url": "/play?url=%s"
    }
  ],
  "edge_side_panel": {
    "preferred_width": 400
  },
  "capture_links": "new-client",
  "launch_handler": {
    "client_mode": "focus-existing"
  }
}
